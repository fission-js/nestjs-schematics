{
    "name": "<%= name %>",
    "version": "<%= version %>",
    "description": "<%= description %>",
    "author": "<%= author %>",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild:back": "rimraf dist",
        "build:back": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start:back": "dotenv nest start",
        "start:back:dev": "dotenv nest start -- --watch",
        "start:back:debug": "dotenv nest start -- --debug --watch",
        "start:back:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\" --fix",
        "test:back": "jest",
        "test:back:watch": "jest --watch",
        "test:back:cov": "jest --coverage",
        "test:back:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:back:e2e": "jest --config ./test/jest-e2e.json",
        "start:front": "react-app-rewired start",
        "build:front": "react-scripts build",
        "test:front": "react-scripts test",
        "eject": "react-scripts eject",
        "db:up": "dotenv docker-compose up -- -d",
        "db:down": "dotenv docker-compose down"
    },
    "dependencies": {
        "@apollo/client": "^3.6.6",
        "@fission-js/fission": "^0.0.23",
        "@nestjs/apollo": "^10.0.12",
        "@nestjs/common": "^8.0.0",
        "@nestjs/core": "^8.0.0",
        "@nestjs/graphql": "^10.0.12",
        "@nestjs/platform-express": "^8.0.0",
        "@nestjs/typeorm": "^8.0.4",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.2.0",
        "@testing-library/user-event": "^14.2.0",
        "antd": "^4.20.5",
        "apollo-server-express": "^3.7.0",
        "graphql": "^16.5.0",
        "pg": "^8.7.3",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-router-dom": "6",
        "rxjs": "^7.2.0",
        "typeorm": "^0.3.6",
        "web-vitals": "^2.1.4"
    },
    "devDependencies": {
        "@nestjs/cli": "^8.0.0",
        "@nestjs/schematics": "^8.0.0",
        "@nestjs/testing": "^8.0.0",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.5.1",
        "@types/node": "^17.0.35",
        "@types/react": "^18.0.9",
        "@types/react-dom": "^18.0.4",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "dotenv": "^16.0.1",
        "dotenv-cli": "^5.1.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.30.0",
        "jest": "^27.2.5",
        "nock": "^13.2.4",
        "prettier": "^2.3.2",
        "react-app-alias": "^2.2.0",
        "react-app-alias-ex": "^2.1.0",
        "react-app-rewired": "^2.2.1",
        "react-scripts": "^5.0.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "^27.0.3",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "^3.10.1",
        "typescript": "^4.6.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
